{
  "name": "fretpilot-studio",
  "version": "1.0.0",
  "description": "Interactive guitar chord trainer for learning and practice",
  "author": "FretPilot Studio",
  "homepage": "https://fretpilotstudio.com",
  "main": "main.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "preflight": "node scripts/preflight-check.js",
    "dns:verify": "node scripts/dns-verify.js fretpilotstudio.com --www",
    "dns:wait": "powershell -ExecutionPolicy Bypass -File scripts/dns-verify.ps1 -Domain fretpilotstudio.com -CheckWWW",
    "check:email-dns": "node scripts/check-email-dns.js fretpilotstudio.com",
    "ios": "npm run build && npx cap sync && npx cap open ios",
    "android": "npm run build && npx cap sync && npx cap open android",
    "electron": "npm run build && npx cap sync && npx cap open electron",
    "electron:build": "npm run build && electron-builder --win --publish never",
    "server": "node server/index.js",
    "compose:temple": "node scripts/compose-temple-slogan.js",
    "launch:verify": "node scripts/launch-verify.js --domain fretpilotstudio.com",
    "stripe:test": "node scripts/stripe-test.js $env:TEST_PRICE_ID",
    "build:icons": "node scripts/generate-icons.js",
    "build:store-assets": "npm install adm-zip && node scripts/generate-icons.js"
  },
  "dependencies": {
    "@capacitor-community/admob": "^5.3.0",
    "@capacitor-firebase/crashlytics": "^5.0.0",
    "@capacitor/android": "^5.5.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.5.0",
    "@capacitor/ios": "^5.5.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@stripe/stripe-js": "^8.5.2",
    "adm-zip": "^0.5.16",
    "chalk": "^4.1.2",
    "cordova-plugin-purchase": "^13.12.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "firebase": "^10.14.1",
    "html2canvas": "^1.4.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.8",
    "sharp": "^0.33.4",
    "stripe": "^16.12.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "vite": "^5.0.0"
  }
}
